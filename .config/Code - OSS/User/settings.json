{
    "clangd.arguments": [
        "--background-index",
        "--compile-commands-dir=build",
        "-j=12",
        "--query-driver=/usr/bin/clang++",
        "--clang-tidy",
        "--all-scopes-completion",
        "--completion-style=detailed",
        "--function-arg-placeholders",
        "--header-insertion=iwyu",
        "--pch-storage=memory",
    ],
    "clangd.detectExtensionConflicts": false,
    "crates.indexServerURL": "https://mirrors.tuna.tsinghua.edu.cn/git/crates.io-index",
    "docker.containers.description": [
        "Ports",
        "Status"
    ],
    "docker.containers.label": "ContainerName",
    "editor.fontFamily": "'FantasqueSansM Nerd Font Mono','LXGW WenKai Neo XiHei','Font Awesome 6 Free'",
    "editor.fontSize": 16,
    "editor.inlayHints.fontFamily": "'FantasqueSansM Nerd Font Mono','LXGW WenKai Neo XiHei','Font Awesome 6 Free'",
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "all",
    "evenBetterToml.formatter.alignComments": true,
    "evenBetterToml.formatter.alignEntries": true,
    "evenBetterToml.formatter.allowedBlankLines": 1,
    "evenBetterToml.formatter.arrayAutoExpand": true,
    "evenBetterToml.formatter.arrayTrailingComma": true,
    "evenBetterToml.formatter.reorderArrays": false,
    "evenBetterToml.formatter.trailingNewline": true,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onWindowChange",
    "files.eol": "\n",
    "files.watcherExclude": {
        "**/.ammonite": true,
        "**/.bloop": true,
        "**/.metals": true
    },
    "git.enableCommitSigning": true,
    "git.showPushSuccessNotification": true,
    "git.terminalGitEditor": true,
    "github.gitProtocol": "ssh",
    "go.diagnostic.vulncheck": "Imports",
    "go.editorContextMenuCommands": {
        "fillStruct": true
    },
    "go.formatTool": "gofumpt",
    "go.inlayHints.assignVariableTypes": true,
    "go.inlayHints.compositeLiteralFields": true,
    "go.inlayHints.compositeLiteralTypes": true,
    "go.inlayHints.constantValues": true,
    "go.inlayHints.functionTypeParameters": true,
    "go.inlayHints.parameterNames": true,
    "go.inlayHints.rangeVariableTypes": true,
    "go.lintTool": "revive",
    "go.showWelcome": false,
    "go.survey.prompt": false,
    "go.toolsManagement.autoUpdate": true,
    "http.proxy": "http://127.0.0.1:7890",
    "lldb.showDisassembly": "never",
    "rust-analyzer.lens.references.adt.enable": true,
    "rust-analyzer.lens.references.enumVariant.enable": true,
    "rust-analyzer.lens.references.method.enable": true,
    "rust-analyzer.lens.references.trait.enable": true,
    "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontFamily": "'FantasqueSansM Nerd Font Mono','LXGW WenKai Neo XiHei','Font Awesome 6 Free'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.rightClickBehavior": "paste",
    "update.mode": "manual",
    "window.autoDetectColorScheme": true,
    "window.commandCenter": true,
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Adwaita Dark",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.preferredDarkColorTheme": "Adwaita Dark",
    "workbench.preferredLightColorTheme": "Adwaita Light",
    "workbench.productIconTheme": "adwaita",
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 12
}