{
    "editor.fontFamily": "'Maple Mono SC NF', 'JetBrainsMono Nerd Font Mono'",
    "editor.fontSize": 16,
    "editor.inlayHints.fontFamily": "'Maple Mono SC NF', 'JetBrainsMono Nerd Font Mono'",
    "editor.renderWhitespace": "all",
    "files.autoSave": "onWindowChange",
    "files.eol": "\n",
    "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.fontFamily": "'Maple Mono SC NF', 'JetBrainsMono Nerd Font Mono'",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.rightClickBehavior": "paste",
    "update.mode": "manual",
    "workbench.sideBar.location": "right",
    "http.proxy": "http://127.0.0.1:7890",
    "clangd.detectExtensionConflicts": false,
    "clangd.arguments": [
        "--background-index",
        "--compile-commands-dir=build",
        "-j=12",
        "--query-driver=/usr/bin/clang++",
        "--clang-tidy",
        "--all-scopes-completion",
        "--completion-style=detailed",
        "--function-arg-placeholders",
        "--header-insertion=iwyu",
        "--pch-storage=memory",
    ],
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "extensions.ignoreRecommendations": true,
    "window.titleBarStyle": "custom",
    "window.dialogStyle": "custom",
    "window.autoDetectColorScheme": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "gitlens.defaultDateLocale": "zh-CN",
    "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "docker.containers.label": "ContainerName",
    "docker.containers.description": [
        "Status"
    ],
}